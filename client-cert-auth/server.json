{
    "name":"commandbox-test-client-cert-auth",
    "web":{
        "bindings":{
            "ssl":{
                "clientCert":{
                    "CACertFiles":"../certs/TestRootCertificate.cer",
                    "catruststorefile":"../certs/TestRootCertificate.pfx",
                    "CATrustStorePass":"test",
                    "mode":"REQUIRED"
                },
                "host":"site1.com",
                "listen":"443"
            }
        },
        "security":{
            "authPredicate":"path-prefix-nocase(secret)",
            "clientCert":{
                "enable":"true",
                "subjectDNs":"CN = aDifferentCommonName"
            }
        }
    }
}
