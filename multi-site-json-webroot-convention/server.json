{
    "name":"commandbox-test-multi-site-json-webroot-convention",
    "web":{
        "accessLogEnable":"true",
        "aliases":{
            "/js":"javascript"
        },
        "allowedExt":"log",
        "errorPages":{
            "404":"404.cfm"
        },
        "GZIPEnable":"true",
        "gzipPredicate":"regex('^.*\\.txt$')",
        "mimeTypes":{
            "log":"text/plain"
        },
        "rules":[
            "path(/tea)->set-error(418)"
        ],
        "rulesFile":".rules.txt",
        "welcomeFiles":"custom.cfm,index.cfm",
        "bindings":{
            "HTTP":{
                "listen":"80"
            }
        }
    },
    "sites":{
        "site1":{
            "webroot":"site1"
        },
        "site2":{
            "webroot":"site2"
        },
        "site3":{
            "webroot":"site3"
        },
        "default":{
            "webroot":"default"
        }
    },
    "hostAlias":"siteMissing.com"
}